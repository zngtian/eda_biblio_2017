}

@article{cooke_regional_1997,
	title = {Regional innovation systems: {Institutional} and organisational dimensions},
	volume = {26},
	issn = {0048-7333},
	shorttitle = {Regional innovation systems: {Institutional} and organisational dimensions},
	abstract = {The paper explores the case for Regional Systems of Innovation. Acknowledging the major contribution of research on National Innovation Systems, it suggests that for conceptual and methodological reasons, mostly concerning problems of scale and complexity, that approach may be complemented in important ways by a subnational focus. Taking an evolutionary economics standpoint, the paper specifies the concepts of 'region,' 'innovation' and 'system' as the prelude to an extended discussion of the importance of financial capacity, institutionalised learning and productive culture to systemic innovation. Building on the notion of regions as occupying different positions on a continuum referring to processes constituting them and their powers vis-à-vis innovation policy, the paper concludes by advocating strengthening of regional level capacities for promoting both systemic learning and interactive innovation.},
	journal = {Research Policy},
	author = {Cooke, Philip and Gomez Uranga, Mikel and Etxebarria, Goio},
	year = {1997},
	keywords = {Innovation, Interaction, Learning, Region, Systems},
	pages = {475--491}
}

